.container{
    padding:0;
    margin:0;
    background-color: #efefef;
}
.swiper-tab{
    width: 100%;
    text-align: center;
    line-height: 60rpx;
    margin-bottom: 20rpx;
    background-color: #fff;
    }
.swiper-tab-list{  
    font-size: 30rpx;
    display: inline-block;
    width: 25%;
    color: #777777;
    box-sizing: border-box;
    padding: 0 40rpx;
}
.swiper-tab-list view {
  height: 70rpx;
  line-height: 70rpx;
  color: #444;
  font-weight: 600;
}
.on view {
 color: #4bd7bd;
    border-bottom: 5rpx solid #4bd7bd;
    }

.swiper-box{
 display: block; 
 height: 100%;
 width: 100%;
 overflow: hidden; 
 margin: 0;
 }
.swiper-box view{
    text-align: left;
}
.page-body{
  height: 100%;
  background-color: #eaeaea;
}
.nav_section_items {
  background-color: #fff;
  padding: 0 20rpx;
  margin-bottom: 20rpx;
}
.nav_section_items .section_cont_tit {
  height: 92rpx;
  border-bottom: 1px solid #bfbfbf;
  line-height: 92rpx;
  color: #444;
  font-size: 34rpx;
  font-weight: 500;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.nav_section_items .section_cont_tit .cartype {
  width: 48rpx;
  height: 48rpx;
}
.nav_section_items .section_cont .addressname {
  line-height: 70rpx;
  color: #7b7b7b;
}
.nav_section_items .section_cont .section_cont_price {
  float: right;
  color: #ff0000;
  font-size: 30rpx;
}
.nav_section_items .section_cont .address {
  color: #6f6f6f;
  width: 560rpx;
  min-height: 80rpx;
  line-height: 40rpx;
  font-size: 30rpx;
}
.nav_section_items .section_cont .ordertime {
  font-size: 24rpx;
  color: #a1a1a1;
  height: 96rpx;
  line-height: 96rpx;
  overflow: hidden;
}

.conCover {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 999;
}
.conCover .conCover-in {
  position: fixed;
  bottom: 10rpx;
  left: 0;
  width: 100%;
  text-align: center;
  background-color: #fff;
  padding: 0 20rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
}
.coverTit {
  height: 100rpx;
  font-size: 36rpx;
  color: #b5b5b5;
  line-height: 100rpx;
}
.conCoverlists {
  height: 260rpx;
  display: flex;
  vertical-align: center;
  border-bottom: 1rpx solid #bfbfbf;
}
.conCoverlists .conCoverlist {
  flex: 1;
  padding-top: 30rpx;
  text-align: center;
}
.conCoverlists view text {
  display: block;
  text-align: center;
  margin: 20rpx 0;
}
.conCover-in .console {
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
}