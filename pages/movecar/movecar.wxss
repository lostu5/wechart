.camer {
  margin: 72rpx auto 0;
  background-color: rgba(0, 0, 0, 0);
  position: relative;
  width: 640rpx;
  height: 380rpx;
}
.camer .topimg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;

}
.mytips {
  display: flex;
  align-items: center;
  justify-content: space-around;
  line-height: 50rpx;
  margin-bottom: 60rpx;
  color: #444;
  font-size: 30rpx;
}
.carcard {
  display: flex;
  align-items: center;
  justify-content: space-around;
  line-height: 60rpxï¼›
}
.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  justify-content: center;
  align-items: center;
  border-right: 1rpx solid #d2d2d2;
}
.td {
    width:60rpx;
    justify-content: center;
    text-align: center;
    height: 80rpx;
    border: 1rpx solid #d2d2d2;
    border-right:none; 
    padding: 0 10rpx;
    line-height: 80rpx;
    box-sizing: border-box;
}
.td input {
  display: block;
  height: 100%;
}
.tdtit {
    width:150rpx;
    justify-content: center;
    text-align: center;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 40rpx;
}

.btn-submit {
  position: fixed;
  left: 0;
  bottom: 10rpx;
  width: 100%;
  padding: 0 20rpx;
  box-sizing: border-box;
  height: 500rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  
}
.btn-submit .btn-t {
  display: block;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
  color: #444;
  text-align: left;
  padding: 0 40rpx;
}
.btn-submit .btn-t .tit {
  font-size: 35rpx;
  padding-left: 30rpx;
  display: inline-block;
}
.btn-submit button{
  height: 80rpx;
  margin-bottom: 20rpx;
  width: 100%;
  color: #fff;
  background-color: #4bd7bd;
  border-radius: 10rpx;
  font-size: 32rpx;
}

.btn-t{
	padding: 0 20rpx;
