.section{
  width: 100%;
}
.mapsearch {
  width: 100%;
  padding:30rpx 20rpx;
  box-sizing: border-box;
}
.mapsearch .mapsearch-in {
  display: flex;
  align-items: center;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #fff;
  border: 1rpx solid #888885;
  border-radius: 10rpx;
  font-size: 30rpx;
  color: #555;
}
.mapsearch .mapsearch-in cover-image {
  width: 44rpx;
  height: 44rpx;
  margin:  0 30rpx 0 40rpx;
}
.map_container{
  position: absolute;
  top: 0rpx;
  bottom: 0rpx;
  left: 0;
  right: 0;
}
.map{
  width: 100%;
  height: 100%;
}

.h1{
  margin: 15px 0;
  font-size:15px;
}
.mapinfo {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  background-color: #fff;
  box-sizing: border-box;
  -webkit-transition: height .2s ease-in;
-moz-transition: height .2s ease-in;
transition: height .2s ease-in;
overflow: none;
}
.mapinfo .gettip {
  background-color: #4bd7bd;
  width: 100rpx;
  height: 50rpx;
  border-radius: 10rpx;
  position: absolute;
  right: 30rpx;
  top: 20rpx;
  font-size: 30rpx;
  line-height: 50rpx;
  color: #fff;
  text-align: center;
}
.mapinfo .mapsearch-in {
  display: block;
  align-items: center;
  padding: 20rpx 20rpx;
  position: relative;
}
.mapinfo .mapsearch-in .title {
  font-size: 36rpx;
  font-weight: 600;
  padding-left: 30rpx;
}
.mapinfo .mapsearch-in cover-image {
  height: 40rpx;
  width: 40rpx;
  padding: 0 10rpx;
}
.mapinfo .mapsearch-in cover-view{
  white-space:pre-wrap;
}
.mapinfo .mapsearch-in .tit {
  float: left;
  display: flex;
  align-items: center;
  justify-content: center
}