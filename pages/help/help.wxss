.ar_coverpath{
    width:100%;
    margin:0;
    box-sizing:border-box;
    height: 30rpx;
    background-color: #efefef;
}
.container {
  width: 100%;
  height: 100%;
  overflow: auto;
  position: relative;
}
.container .bg {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
}
.lists {
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 99;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.lists .list {
  font-size: 36rpx;
  display: flex;
  align-items: center;
  flex-direction:column;
  padding: 20rpx 0;
}
.lists .list .title {
  font-size: 32rpx;
  color: #fff;
  margin-bottom: 30rpx;
  text-indent: 60rpx;
}
