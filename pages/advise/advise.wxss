.ar_coverpath{
    width:100%;
    margin:0;
    box-sizing:border-box;
    height: 30rpx;
    background-color: #efefef;
}
page {
  height: 100%;
}
.container {
  width: 100%;
  height: 100%;
  position: relative;
}
.container .bg {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
}
.lists {
  width: 100%;
  padding: 0 20rpx;
  box-sizing: border-box;
  position: absolute;
  left: 0;
  top: 0;
  padding-top: 20rpx;
}
.lists  .title {
  height: 90rpx;
  line-height: 90rpx;
  font-size: 36rpx;
  color: #fff;
  border-bottom: 1rpx solid #efefef;
}
.lists  .tip {
  font-size: 30rpx;
  color: #efefef;
  padding: 30rpx 0;
}
.phonenum {
  font-size: 30rpx;
  color: #fff;
  width: 100%;
  float: left;
  margin-bottom: 20rpx;
}
.phonenum .phonet {
  float: left;
}
.phonenum .phonen {
  float: left;
}